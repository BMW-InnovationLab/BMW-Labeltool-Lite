  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "ng serve --prod",
    "start:mock": "ng serve -c mock",
    "build": "ng build --named-chunks",
    "build:prod": "ng build --prod --no-progress --named-chunks",
