{
  "name": "labeltool",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "ng serve --prod",
    "start:mock": "ng serve -c mock",
    "build": "ng build --named-chunks",
    "build:prod": "ng build --prod --no-progress --named-chunks",
    "lint": "npm run lint:ts",
    "lint:ts": "ng lint labeltool",
    "lint:scss": "stylelint \"**/*.scss\" --config .stylelintrc.json",
    "format": "prettier --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss,.html}\"",
    "precommit": "npm run lint && pretty-quick --staged",
    "prepush": "npm run build:prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.5",
    "@angular/cdk": "^7.3.2",
    "@angular/common": "7.2.5",
    "@angular/compiler": "7.2.5",
    "@angular/core": "7.2.5",
    "@angular/forms": "7.2.5",
    "@angular/http": "7.2.5",
    "@angular/material": "^7.3.2",
    "@angular/platform-browser": "7.2.5",
    "@angular/platform-browser-dynamic": "7.2.5",
    "@angular/router": "7.2.5",
    "@ngxs-labs/immer-adapter": "^1.1.1",
    "@ngxs/devtools-plugin": "^3.3.2",
    "@ngxs/router-plugin": "^3.3.2",
    "@ngxs/store": "^3.3.2",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "immer": "^1.5.0",
    "normalize-wheel": "^1.0.1",
    "parse-color": "^1.0.0",
    "rxjs": "^6.3.3",
    "shim-keyboard-event-key": "^1.0.2",
    "svg.draggable.js": "^2.2.2",
    "svg.js": "^2.6.4",
    "svg.resize.js": "^1.4.1",
    "svg.select.js": "github:svgdotjs/svg.select.js#master",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.11.3",
    "@angular/cli": "7.0.4",
    "@angular/compiler-cli": "7.2.5",
    "@angular/language-service": "7.2.5",
    "@types/node": "^6.0.101",
    "codelyzer": "~4.5.0",
    "husky": "^0.14.3",
    "prettier": "^1.15.1",
    "pretty-quick": "^1.6.0",
    "stylelint": "^9.2.1",
    "stylelint-config-sass-guidelines": "^5.0.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^2.4.0",
    "ts-node": "~3.2.0",
    "tsickle": "^0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.10.0",
    "typescript": "3.1.6"
  }
}
